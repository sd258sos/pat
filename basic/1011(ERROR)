#include <cstdio>
int main()
{

	int n;
	int a,b,c;
	int k[100];
	scanf("%d",&n);
	for(int i=0;i<n;i++)
	{
		scanf("%d %d %d",&a,&b,&c);
		if((a+b)>c)
		{
			k[i]=1;
		}else{
			k[i]=0;
		}
	
	}
	for(int j=0;j<n;j++)
	{
		if(k[j]==1)
		{
		printf("Case #%d: true\n",(j+1));
		}else{
		printf("Case #%d: false\n",(j+1));
		}
	}


    return 0;
}
