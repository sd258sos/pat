#include <iostream>
using namespace std;
/*
1.输入N个数
2.输入N个不同的数
3.组成两位数并求和
4.输出求和的值 
*/

int main(){
    int n,temp,sum=0;
    
    scanf("%d",&n);
    for(int i=0;i<n;i++){
            scanf("%d",&temp);
            sum=sum+temp*(n-1)*10+temp*(n-1);
            }
    printf("%d", sum);
    return 0;
    } 
